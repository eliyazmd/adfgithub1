{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eliyazadf1"
		},
		"ls_adfdlsg1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adfdlsg1'"
		},
		"blobjson_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobjson'"
		},
		"adlsgen2json_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsgen2json'"
		},
		"LS_Sql_Connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Sql_Connection'"
		},
		"ls_blob1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ConnectionString"
		},
		"ls_adfdlsg1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfakkidlsg1.dfs.core.windows.net"
		},
		"adlsgen2json_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfakkidlsg1.dfs.core.windows.net"
		},
		"Ls_HTTP_Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azurenewkeyvault01akki.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "eliyazadf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adfdlsg1_accountKey": {
						"value": "[parameters('ls_adfdlsg1_accountKey')]"
					},
					"blobjson_connectionString": {
						"value": "[parameters('blobjson_connectionString')]"
					},
					"adlsgen2json_accountKey": {
						"value": "[parameters('adlsgen2json_accountKey')]"
					},
					"LS_Sql_Connection_connectionString": {
						"value": "[parameters('LS_Sql_Connection_connectionString')]"
					},
					"ls_adfdlsg1_properties_typeProperties_url": {
						"value": "[parameters('ls_adfdlsg1_properties_typeProperties_url')]"
					},
					"adlsgen2json_properties_typeProperties_url": {
						"value": "[parameters('adlsgen2json_properties_typeProperties_url')]"
					},
					"Ls_HTTP_Connection_properties_typeProperties_url": {
						"value": "[parameters('Ls_HTTP_Connection_properties_typeProperties_url')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "eliyazadf1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_blob1_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'eliyazadf1_ArmTemplate_0')]"
			]
		},
		{
			"name": "eliyazadf1_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'eliyazadf1_ArmTemplate_1')]"
			]
		}
	]
}